SELECT s.id, s.provider_id, s.detail_id, s.project_id
FROM task2.supplies s
JOIN task2.providers pro
ON pro.provider_id = s.provider_id
JOIN task2.projects pr
ON pr.project_id = s.project_id
JOIN task2.details d
ON d.detail_id = s.detail_id 
WHERE pro.city <> d.city AND pro.city <> pr.city AND d.city <> pr.city

